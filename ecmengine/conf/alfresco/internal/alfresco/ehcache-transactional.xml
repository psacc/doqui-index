<!--
   This configures the cache properties for Alfresco's in-transaction caches.
   Do not cluster this.
-->
<ehcache>
    <diskStore 
        path="java.io.tmpdir"/>
    <defaultCache 
        maxElementsInMemory="5000" 
        eternal="true" 
        timeToIdleSeconds="0" 
        timeToLiveSeconds="0" 
        overflowToDisk="false" 
        />
</ehcache>