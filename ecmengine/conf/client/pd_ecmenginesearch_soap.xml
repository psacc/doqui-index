	<config>
		<url-pa></url-pa>
		<pluggable-pd-list
			chooser-class="it.csi.csi.porte.FixedDelegateChooser">
			<pluggable-pd name="pa_ecmenginesearch_soap"
				class="it.csi.csi.porte.soap.apache.PortaDelegataSoap"
				url-pa="http://@@url_soap@@/servlet/rpcrouter">
				<properties />
			</pluggable-pd>
		</pluggable-pd-list>
		<queue-manager url="" />
	</config>
</porta-delegata>
