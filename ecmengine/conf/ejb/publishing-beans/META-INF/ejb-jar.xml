<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
    <enterprise-beans>
        <session>
            <ejb-name>EcmEngineManagementBean@@ejbextension@@</ejb-name>
            <home>it.doqui.index.ecmengine.business.publishing.management.EcmEngineManagementHome</home>
            <remote>it.doqui.index.ecmengine.business.publishing.management.EcmEngineManagement</remote>
            <ejb-class>it.doqui.index.ecmengine.business.publishing.management.EcmEngineManagementBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <ejb-name>EcmEngineSearchBean@@ejbextension@@</ejb-name>
            <home>it.doqui.index.ecmengine.business.publishing.search.EcmEngineSearchHome</home>
            <remote>it.doqui.index.ecmengine.business.publishing.search.EcmEngineSearch</remote>
            <ejb-class>it.doqui.index.ecmengine.business.publishing.search.EcmEngineSearchBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
        	<ejb-name>EcmEngineBackofficeBean@@ejbextension@@</ejb-name>
        	<home>it.doqui.index.ecmengine.business.publishing.backoffice.EcmEngineBackofficeHome</home>
        	<remote>it.doqui.index.ecmengine.business.publishing.backoffice.EcmEngineBackoffice</remote>
        	<ejb-class>it.doqui.index.ecmengine.business.publishing.backoffice.EcmEngineBackofficeBean</ejb-class>
        	<session-type>Stateless</session-type>
        	<transaction-type>Container</transaction-type>
        </session>
    </enterprise-beans>
    <assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>EcmEngineManagementBean@@ejbextension@@</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>NotSupported</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <ejb-name>EcmEngineSearchBean@@ejbextension@@</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>NotSupported</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <ejb-name>EcmEngineBackofficeBean@@ejbextension@@</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>NotSupported</trans-attribute>
        </container-transaction> 	
    </assembly-descriptor>
</ejb-jar>