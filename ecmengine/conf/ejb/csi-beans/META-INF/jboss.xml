<!DOCTYPE jboss PUBLIC "-//JBoss//DTD JBOSS 4.0//EN" "http://www.jboss.org/j2ee/dtd/jboss_4_0.dtd">
<jboss>
	<enterprise-beans>
		<session>
			<ejb-name>pa_ecmenginemanagement@@ejbextension@@</ejb-name>
			<jndi-name>ecmengine/ejb/ManagementBeanPA@@ejbextension@@</jndi-name>
			<clustered>@@clustered@@</clustered>
			<cluster-config>
				<partition-name>@@cluster_partition@@</partition-name>
			</cluster-config>
		</session>
		<session>
			<ejb-name>pa_ecmenginesearch@@ejbextension@@</ejb-name>
			<jndi-name>ecmengine/ejb/SearchBeanPA@@ejbextension@@</jndi-name>
			<clustered>@@clustered@@</clustered>
			<cluster-config>
				<partition-name>@@cluster_partition@@</partition-name>
			</cluster-config>
		</session>
		<session>
			<ejb-name>pa_ecmenginebackoffice@@ejbextension@@</ejb-name>
			<jndi-name>ecmengine/ejb/BackofficeBeanPA@@ejbextension@@</jndi-name>
			<clustered>@@clustered@@</clustered>
			<cluster-config>
				<partition-name>@@cluster_partition@@</partition-name>
			</cluster-config>
		</session>	
	</enterprise-beans>
</jboss>