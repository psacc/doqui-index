<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
	<enterprise-beans>
        <session>
            <ejb-name>pa_ecmenginemanagement@@ejbextension@@</ejb-name>
            <home>it.csi.csi.porte.ejb20.PortaApplicativaHome</home>
            <remote>it.csi.csi.porte.ejb20.PortaApplicativaEJB</remote>
            <ejb-class>it.csi.csi.porte.ejb20.PortaApplicativaBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
            <env-entry>
                <description />
                <env-entry-name>defpa</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>/pa_ecmenginemanagement.xml</env-entry-value>
            </env-entry>
        </session>
        <session>
            <ejb-name>pa_ecmenginesearch@@ejbextension@@</ejb-name>
            <home>it.csi.csi.porte.ejb20.PortaApplicativaHome</home>
            <remote>it.csi.csi.porte.ejb20.PortaApplicativaEJB</remote>
            <ejb-class>it.csi.csi.porte.ejb20.PortaApplicativaBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
            <env-entry>
                <description />
                <env-entry-name>defpa</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>/pa_ecmenginesearch.xml</env-entry-value>
            </env-entry>
        </session>
        <session>
            <ejb-name>pa_ecmenginebackoffice@@ejbextension@@</ejb-name>
            <home>it.csi.csi.porte.ejb20.PortaApplicativaHome</home>
            <remote>it.csi.csi.porte.ejb20.PortaApplicativaEJB</remote>
            <ejb-class>it.csi.csi.porte.ejb20.PortaApplicativaBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
            <env-entry>
                <description />
                <env-entry-name>defpa</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>/pa_ecmenginebackoffice.xml</env-entry-value>
            </env-entry>
        </session>		
    </enterprise-beans>
    <assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>pa_ecmenginemanagement@@ejbextension@@</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>RequiresNew</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <ejb-name>pa_ecmenginesearch@@ejbextension@@</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>RequiresNew</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <ejb-name>pa_ecmenginebackoffice@@ejbextension@@</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>RequiresNew</trans-attribute>
        </container-transaction>
    </assembly-descriptor>
</ejb-jar>