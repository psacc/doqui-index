<!DOCTYPE jboss PUBLIC "-//JBoss//DTD JBOSS 4.0//EN" "http://www.jboss.org/j2ee/dtd/jboss_4_0.dtd">
<jboss>
	<enterprise-beans>
		<session>
			<ejb-name>EcmEngineSecurityBean@@ejbextension@@</ejb-name>
			<jndi-name>ecmengine/ejb/SecurityBean@@ejbextension@@</jndi-name>
			<clustered>@@clustered@@</clustered>
			<cluster-config>
				<partition-name>@@cluster_partition@@</partition-name>
			</cluster-config>
		</session>
        @@exclude_pa_on@@
		<session>
			<ejb-name>pa_ecmenginesecurity@@ejbextension@@</ejb-name>
			<jndi-name>ecmengine/ejb/SecurityBeanPA@@ejbextension@@</jndi-name>
			<clustered>@@clustered@@</clustered>
			<cluster-config>
				<partition-name>@@cluster_partition@@</partition-name>
			</cluster-config>
		</session>
        @@exclude_pa_off@@
	</enterprise-beans>
</jboss>
