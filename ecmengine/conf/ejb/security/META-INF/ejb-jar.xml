<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
    <enterprise-beans>
        <session>
        	<ejb-name>EcmEngineSecurityBean@@ejbextension@@</ejb-name>
        	<home>it.doqui.index.ecmengine.business.publishing.security.EcmEngineSecurityHome</home>
        	<remote>it.doqui.index.ecmengine.business.publishing.security.EcmEngineSecurity</remote>
        	<ejb-class>it.doqui.index.ecmengine.business.publishing.security.EcmEngineSecurityBean</ejb-class>
        	<session-type>Stateless</session-type>
        	<transaction-type>Container</transaction-type>
        </session>
        @@exclude_pa_on@@
        <session>
            <ejb-name>pa_ecmenginesecurity@@ejbextension@@</ejb-name>
            <home>it.csi.csi.porte.ejb20.PortaApplicativaHome</home>
            <remote>it.csi.csi.porte.ejb20.PortaApplicativaEJB</remote>
            <ejb-class>it.csi.csi.porte.ejb20.PortaApplicativaBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
            <env-entry>
                <description />
                <env-entry-name>defpa</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>/pa_ecmenginesecurity.xml</env-entry-value>
            </env-entry>
        </session>
        @@exclude_pa_off@@
    </enterprise-beans>
    <assembly-descriptor>
		<container-transaction>
            <method>
                <ejb-name>EcmEngineSecurityBean@@ejbextension@@</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>NotSupported</trans-attribute>
        </container-transaction>
        @@exclude_pa_on@@
        <container-transaction>
            <method>
                <ejb-name>pa_ecmenginesecurity@@ejbextension@@</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>RequiresNew</trans-attribute>
        </container-transaction>
        @@exclude_pa_off@@
    </assembly-descriptor>
</ejb-jar>
